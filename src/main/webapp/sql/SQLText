//入住企业表
DROP TABLE IF EXISTS `stay_in_business`;
CREATE TABLE `stay_in_business` (
  `pk_stay_in_business` BIGINT(20) NOT NULL AUTO_INCREMENT COMMENT '记录ID',
  `name` VARCHAR(50) NOT NULL COMMENT '企业名称',
  `type` INT(2) NOT NULL COMMENT '企业类型',
  `person_in_charge` VARCHAR(25) NOT NULL COMMENT '企业负责人',
  `address` VARCHAR(60) NOT NULL COMMENT '办公地址',
  `telephone` VARCHAR(18) NOT NULL COMMENT '企业电话',
  `memo` VARCHAR(500) DEFAULT NULL COMMENT '企业介绍',
  `def1` VARCHAR(255) DEFAULT NULL,
  `def2` VARCHAR(255) DEFAULT NULL,
  `def3` VARCHAR(255) DEFAULT NULL,
  `def4` VARCHAR(255) DEFAULT NULL,
  `def5` VARCHAR(255) DEFAULT NULL,
  `bd_std` CHAR(1) DEFAULT NULL,
  `dr` INT(1) DEFAULT NULL,
  `ts` TIMESTAMP NULL DEFAULT NULL ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`pk_stay_in_business`)
) ENGINE=INNODB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8;

//园区表
DROP TABLE IF EXISTS `park_archives`;
CREATE TABLE `park_archives` (
  `pk_park_archives` BIGINT(20) NOT NULL AUTO_INCREMENT COMMENT '记录ID',
  `park_name` VARCHAR(50) NOT NULL COMMENT '园区名称',
  `park_code` VARCHAR(50) NOT NULL COMMENT '园区编码',
  `park_type` INT(2) NOT NULL COMMENT '园区类型',
  `build_area` VARCHAR(25) DEFAULT NULL COMMENT '建筑面积',
  `area_covered` VARCHAR(60) DEFAULT NULL COMMENT '占地面积',
  `building_quantity` INT(2) NOT NULL COMMENT '楼宇数量',
  `parking_spaces_quantity` INT(2) DEFAULT NULL COMMENT '车位数量',
  `person_in_charge` VARCHAR(25) DEFAULT NULL COMMENT '负责人',
  `phone` VARCHAR(11) DEFAULT NULL COMMENT '联系电话',
  `memo` VARCHAR(500) DEFAULT NULL COMMENT '园区介绍',
  `def1` VARCHAR(255) DEFAULT NULL,
  `def2` VARCHAR(255) DEFAULT NULL,
  `def3` VARCHAR(255) DEFAULT NULL,
  `def4` VARCHAR(255) DEFAULT NULL,
  `def5` VARCHAR(255) DEFAULT NULL,
  `bd_std` CHAR(1) DEFAULT NULL,
  `dr` INT(1) DEFAULT NULL,
  `ts` TIMESTAMP NULL DEFAULT NULL ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`pk_park_archives`)
) ENGINE=INNODB DEFAULT CHARSET=utf8